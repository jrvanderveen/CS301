#lang racket

(define (list-to-set l)
    (cond [(null? l) '()]
          [(member(car l)(cdr l))
           (list-to-set(cdr l))]
          (else(cons(car l) (list-to-set(cdr l))))))
      
(define union
  (lambda(l1 l2)
    (cond [(null? l1) l2]
          [(member(car l1) l2)
           (union(cdr l1) l2)]
          (else(cons(car l1) (union(cdr l1) l2))))))    

(define intersection
  (lambda(l1 l2)
    (cond [(null? l1) '()]
          [(member(car l1) l2)
           (cons(car l1) (intersection(cdr l1) l2))]
          (else(intersection(cdr l1) l2)))))